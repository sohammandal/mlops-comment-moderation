# .env.example
# Copy this file to .env and fill in the appropriate values

# URL for your ECR repository (Terraform will inject the actual value for EC2)
ECR_REPOSITORY_URL=your_ecr_repository_url

# Path or name of the model to use for inference
# Can be a HuggingFace model name (e.g., unitary/toxic-bert)
# or an S3 path to a trained AutoML model (e.g., s3://mlops-comment-artifacts/models/latest.joblib)
MODEL_PATH=unitary/toxic-bert

# AWS profile to use for CLI and boto3 calls (credentials must exist in ~/.aws/credentials)
AWS_PROFILE=mlops